#problem_1
def solution(new_id):
    new_id = new_id.lower()   ## 1단계

    ch = list("abcdefghijklmnopqrstuvwxyz0123456789-_,.")     ####2단계



    new_id = new_id.strip('.')   ###4단계

    if new_id == '':    ###5단계
        new_id = 'a'

    new_id = new_id[:15]     ###6단계
    

